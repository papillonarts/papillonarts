{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": true,
		"noEmitOnError": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": false,
		"target": "es5",
		"outDir": "build",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"jsx": "react-jsx"
	},
	"include": ["src/**/*"],
	"exclude": ["**/*.test.ts"]
}
